{
  "$comment": "app",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "port": {
      "type": "number"
    },
    "ssl": {
      "type": "object",
      "properties": {
        "sslKeyPath": {
          "type": "string"
        },
        "sslCertPath": {
          "type": "string"
        }
      },
      "required": [ "sslKeyPath", "sslCertPath" ]
    }
  },
  "required": [
    "name",
    "port"
  ]
}
